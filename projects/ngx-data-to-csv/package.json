{
  "name": "ngx-data-to-csv",
  "version": "1.0.7",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/ngx-data-to-csv/"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "peerDependencies": {
    "@angular/common": "^15.2.0",
    "@angular/core": "^15.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "sideEffects": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/high54/angular-libraries/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/high54/angular-libraries.git"
  },
  "author": {
    "name": "Julien Bertacco"
  },
  "keywords": [
    "Angular",
    "Ngx Data To CSV",
    "CSV export",
    "Data transformation",
    "Angular service",
    "Downloadable CSV",
    "NPM package",
    "JSON to CSV conversion",
    "Configurable CSV options",
    "Angular CLI",
    "Data export"
  ]
}
